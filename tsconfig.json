{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "types": ["expo-router"],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    ".expo/types/**/*.tsx",
    "expo-env.d.ts"
  ],
  "exclude": ["node_modules"]
}